{
	"name": "CSS-Modules-transform",
	"displayName": "CSS-Modules-transform",
	"description": "CSS-Modules-transform",
	"icon":"icon.png",
	"publisher": "jingxiu",
  "repository": {
    "type": "git",
    "url": "https://github.com/Java-http/CSS-Modules-transform-vsc"
  },
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:CSS-Modules-transform.cssModulesTransform"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "CSS-Modules-transform.cssModulesTransform",
				"title": "cssModulesTransform"
			}
		],
		"menus":{
			"editor/context":[
				{
					"when": "editorHasSelection",
					"command": "CSS-Modules-transform.cssModulesTransform",
					"alt": "CSS-Modules-transform.cssModulesTransform",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
